{
  "name": "@clothesline/icons",
  "version": "0.0.1",
  "type": "module",
  "//": "Entry points for consumers",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "//2": "Modern exports map",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./components/*": "./dist/components/*"
  },
  "//3": "Only publish built artifacts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist src/index.ts",
    "build": "npm run clean && node ./src/scripts/build-icons.mjs && tsup src/index.ts --format esm --dts --out-dir dist && cpx \"src/components/**/*\" dist/components"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^24.10.1",
    "cpx": "^1.5.0",
    "fast-glob": "^3.3.3",
    "rimraf": "^6.0.1",
    "svelte": "^5.45.2",
    "svgo": "^3.3.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.6.0"
  }
}
